{
  "name": "frock-static",
  "version": "0.1.0",
  "description": "A static file server for frock",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm test",
    "lint": "standard src/**/*.js tests/**/*.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "start": "npm run build -- --watch",
    "test": "npm run build && babel-node tests/index.js &&  npm run lint"
  },
  "author": "Urban Airship",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "core-js": "^1.1.4",
    "mime-types": "^2.1.4",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "rimraf": "^2.4.3",
    "standard": "^5.1.0",
    "tape": "^4.0.0"
  },
  "frock": {
    "compatible-versions": "0.1.0 - 0.9.*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
