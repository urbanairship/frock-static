{
  "name": "frock-static",
  "version": "0.1.1",
  "description": "A static file server for frock",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm test",
    "lint": "standard src/**/*.js tests/**/*.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "start": "npm run build -- --watch",
    "test": "npm run build && babel-node tests/index.js &&  npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urbanairship/frock-static.git"
  },
  "author": "Urban Airship",
  "contributors": [
    "Nathan Wittstock <nate.wittstock@urbanairship.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/urbanairship/frock-static/issues"
  },
  "homepage": "https://github.com/urbanairship/frock-static#readme",
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "mime-types": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-preset-es2015": "^6.0.15",
    "node-mocks-http": "^1.5.1",
    "proxyquire": "^1.7.3",
    "rimraf": "^2.4.3",
    "standard": "^5.1.0",
    "tape": "^4.0.0",
    "through": "^2.3.8"
  },
  "frock": {
    "compatible-versions": "0.1.0 - 0.9.*"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
